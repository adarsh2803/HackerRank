// Complete the mergeLists function below.

/*
 * For your reference:
 *
 * SinglyLinkedListNode {
 *     int data;
 *     SinglyLinkedListNode* next;
 * };
 *
 */
 SinglyLinkedListNode* mergeLists(SinglyLinkedListNode* head1, SinglyLinkedListNode* head2) {
    int i=0,j=0;
    SinglyLinkedListNode *p=head1;
    SinglyLinkedListNode *q=head2;
     SinglyLinkedListNode *new_head=NULL, *sorting=NULL;
    if(p==NULL)return q;
    if(q==NULL)return p;
    if(p &&q){
        if(p->data<=q->data) {sorting=p;p=p->next;}
        else {sorting=q;q=q->next;}

    }
    new_head=sorting;
    while(p&&q)
    {
        if(p->data<=q->data){
            sorting->next=p;
            sorting=p;
            p=sorting->next;
        }
        else{
            sorting->next=q;
            sorting=q;
            q=sorting->next;
        }
    }
    if(p==NULL)sorting->next=q;
    if(q==NULL)sorting->next=p;

    return new_head;
}
